{
  "name": "plan2net/a11y-external-link",
  "type": "typo3-cms-extension",
  "description": "Adds accessibility features to external links",
  "version": "1.0.0",
  "require": {
    "php": "^8.2",
    "typo3/cms-core": "^12.4 || ^13.4",
    "typo3/cms-frontend": "^12.4 || ^13.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Plan2net\\ExternalLinkAccessibility\\": "Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plan2net\\ExternalLinkAccessibility\\Tests\\": "Tests/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "external_link_accessibility"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  }
}
